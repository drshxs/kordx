local http_request = request or (syn and syn.request);
if not http_request or not setclipboard then
game.Players.LocalPlayer:Kick("Executer not supported.")
return; -- // break out of script
end
local hwid = http_request({
  Url = "https://luaguard.rocks/Roblox/hwid.php",
  Method = 'GET'
}).Body;

hwidlink = "https://raw.githubusercontent.com/drshxs/kordx/main/ghsehs" -- Link To HWIDS
banlink = "https://raw.githubusercontent.com/drshxs/kordx/main/jtjfjf" --Link To Bans
hwids = game:HttpGet(hwidlink, true)
banlist = game:HttpGet(banlink, true)
current = hwid
if hwids == hwid then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/drshxs/kordx/main/dwadw", true))()
elseif banlist == hwid then
    game.Players.LocalPlayer:Kick("User blacklisted!")
end
